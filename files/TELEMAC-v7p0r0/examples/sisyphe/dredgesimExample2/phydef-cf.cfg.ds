    2    1    0 sea_water_x_velocity
    2    2    0 sea_water_y_velocity
    2    3    0 upward_sea_water_velocity
    3    0    0 sea_surface_height
    5    0    0 sea_water_salinity
    7    0    0 concentration_of_suspended_matter_in_sea_water
  617    0    0 sea_floor_depth_below_sea_level
   17    0    0 sea_floor_depth
    0    0    0 names_of_suspended_matter_in_sea_water
    0    0    0 projection_x_coordinate
    0    0    0 projection_y_coordinate
